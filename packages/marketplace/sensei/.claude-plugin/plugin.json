{
	"agents": ["./agents/sensei.md"],
	"author": {
		"name": "Alizain Feerasta"
	},
	"description": "Documentation research agent with Scout for repository exploration",
	"keywords": [
		"scout",
		"kura",
		"repo-map",
		"code-exploration",
		"documentation",
		"research",
		"cache"
	],
	"license": "MIT",
	"mcpServers": {
		"kura": {
			"headers": {
				"Authorization": "Bearer ${SENSEI_API_KEY}"
			},
			"type": "http",
			"url": "${SENSEI_HOST:-http://localhost:8000}/kura/mcp"
		},
		"scout": {
			"headers": {
				"Authorization": "Bearer ${SENSEI_API_KEY}"
			},
			"type": "http",
			"url": "${SENSEI_HOST:-http://localhost:8000}/scout/mcp"
		}
	},
	"name": "sensei",
	"repository": "https://github.com/alizain/sensei",
	"version": "1.0.0"
}
